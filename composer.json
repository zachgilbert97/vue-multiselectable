{
    "name": "zachgilbert/vue-multiselectable",
    "description": "PHP support for vue-multiselect",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Zach Gilbert",
            "email": "zilberdev@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "ZachGilbert\\VueMultiselectable\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ZachGilbert\\VueMultiselectable\\Tests\\": "tests"
        }
    },
    "require": {
        "illuminate/database": "^6.0|^7.0|^8.0",
        "illuminate/support": "^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.10",
        "mockery/mockery": "^1.4.4"
    },
    "extra": {
        "laravel": {
            "providers": [
                "ZachGilbert\\VueMultiselectable\\Providers\\MultiselectProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
